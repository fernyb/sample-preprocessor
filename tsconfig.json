{
  "compilerOptions": {
    "strict": true,
    "baseUrl": "cypress",
    "paths": {
      "controllers/*": ["fixtures/controllers/*"]
    },
    "target": "es2016",
    "types": [],
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "module": "commonjs",
    "moduleResolution": "Node",
    "declaration": true
    // "outDir": "./dist",
  },
  "include": [
    "**/*.ts"
  ]
}
